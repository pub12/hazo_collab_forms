{
  "name": "hazo_collab_forms",
  "version": "1.1.3",
  "description": "Collaboration form elements",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.client.js",
      "types": "./dist/index.client.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "hazo-collab-forms-verify": "./dist/bin/verify-setup.js",
    "hazo-collab-forms-setup": "./dist/bin/setup-shadcn.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev:package": "tsc -p tsconfig.build.json --watch",
    "dev:test-app": "npm run build && cd test-app && npm run dev",
    "build:test-app": "npm run build && cd test-app && npm run build",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "postinstall": "node dist/bin/postinstall.js 2>/dev/null || true"
  },
  "keywords": [
    "collaboration",
    "forms"
  ],
  "author": "Pubs Abayasiri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pub12/hazo_collab_forms.git"
  },
  "bugs": {
    "url": "https://github.com/pub12/hazo_collab_forms/issues"
  },
  "homepage": "https://github.com/pub12/hazo_collab_forms#readme",
  "workspaces": [
    "test-app"
  ],
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "hazo_auth": "^4.0.0",
    "hazo_config": "^1.0.0",
    "hazo_ui": "^2.2.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.2.0"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "hazo_chat": "^3.0.0",
    "lucide-react": ">=0.344.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-icons": "^5.0.0",
    "sonner": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "lucide-react": {
      "optional": true
    }
  }
}
